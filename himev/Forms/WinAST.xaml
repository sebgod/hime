<Window x:Class="Hime.HimeV.Forms.WinAST"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Parse Result" Height="300" Width="300">
    <DockPanel LastChildFill="True">
        <Expander Header="Errors" DockPanel.Dock="Bottom">
            <ScrollViewer >
                <Grid Name="ErrorGrid"></Grid>
            </ScrollViewer>
        </Expander>
        <ScrollViewer>
            <TreeView Name="Tree">
            </TreeView>
        </ScrollViewer>
    </DockPanel>
</Window>
